{
  "name": "@nestlib/utils",
  "version": "4.0.0",
  "description": "LSK.js – nest utils – helpers and utils for Nest.js projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "dependencies": {
    "@lsk4/algos": "^4.1.0",
    "@lsk4/env": "^4.1.0",
    "@lsk4/err": "^4.1.0",
    "@lsk4/log": "^4.1.0",
    "@lskjs/webserver": "^3.17.0",
    "@types/express": "^4.17.17",
    "fast-safe-stringify": "2.1.1",
    "nanoid": "^3.3.6"
  },
  "devDependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "express": "4.18.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "_size-limit": [
    {
      "path": "lib/AnyExceptionFilter.js",
      "limit": "10kb"
    },
    {
      "path": "lib/AccessLoggerMiddleware.js",
      "limit": "10kb"
    },
    {
      "path": "lib/AnyExceptionFilter.js",
      "limit": "10kb"
    },
    {
      "path": "lib/createNestLogger.js",
      "limit": "10kb"
    },
    {
      "path": "lib/Time.decorator.js",
      "limit": "10kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-nest/nest-utils",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs/nest-utils",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
