{
  "name": "@nestlib/rabbitmq",
  "version": "4.25.0",
  "description": "Nestlib – Nest rabbitmq – helpers for rabbitmq in NestJS projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@types/amqplib": "^0.10.4",
    "@types/node": "^20.11.10"
  },
  "peerDependencies": {
    "@nestjs/common": "*",
    "@nestjs/core": "*"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.7.0",
    "@lsk4/algos": "^4.9.0",
    "@lsk4/env": "^4.6.0",
    "@lsk4/err": "^4.9.0",
    "@lsk4/log": "^4.9.0",
    "amqplib": "^0.10.3",
    "fishbird": "^1.1.8",
    "rxjs": "^7.8.1"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "_size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/nestlibs/nestlib/tree/main/libs/rabbitmq",
  "homepage": "https://github.com/nestlibs/nestlib/tree/main/libs/rabbitmq",
  "bugs": "https://github.com/nestlibs/nestlib/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
