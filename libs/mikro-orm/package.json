{
  "name": "@nestlib/mikro-orm",
  "version": "4.21.0",
  "description": "Nestlib – Nest MikroORM – logger factory for MikroORM in NestJS projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "dependencies": {
    "@lsk4/log": "^4.9.0"
  },
  "devDependencies": {
    "@mikro-orm/core": "^6.0.4"
  },
  "peerDependencies": {
    "@mikro-orm/core": "*"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/nestlibs/nestlib/tree/master/libs/mikro-orm",
  "homepage": "https://github.com/nestlibs/nestlib/tree/master/libs/mikro-orm",
  "bugs": "https://github.com/nestlibs/nestlib/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
