{
  "name": "@nestlib/cache",
  "version": "4.23.2",
  "description": "Nestlib – Nest cache – helpers for cache in NestJS projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "dependencies": {
    "@lsk4/env": "^4.6.0",
    "@lsk4/err": "^4.9.0",
    "@lsk4/log": "^4.9.0",
    "lru-cache": "^10.0.0",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestlib/mutex": "^4.21.0"
  },
  "peerDependencies": {
    "@nestjs/cache-manager": "*",
    "@nestjs/common": "*",
    "@nestjs/core": "*",
    "@nestlib/mutex": "*"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "_size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/nestlibs/nestlib/tree/main/libs/cache",
  "homepage": "https://github.com/nestlibs/nestlib/tree/main/libs/cache",
  "bugs": "https://github.com/nestlibs/nestlib/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
