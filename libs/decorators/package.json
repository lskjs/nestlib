{
  "name": "@nestlib/decorators",
  "version": "4.35.3",
  "description": "Nestlib – Nest decorators – custom parameter decorators and transformers for NestJS projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            ycmd dev",
    "build": "          ycmd build",
    "test": "           ycmd test",
    "prepack": "        ycmd prepack",
    "release": "        ycmd release"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lsk4/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lsk4/eslint-config"
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./cjs/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./cjs/*.d.ts",
      "default": "./cjs/*.js"
    },
    "./transformers": {
      "import": "./lib/transformers/index.js",
      "types": "./cjs/transformers/index.d.ts",
      "default": "./cjs/transformers/index.js"
    },
    "./transformers/*": {
      "import": "./lib/transformers/*.js",
      "types": "./cjs/transformers/*.d.ts",
      "default": "./cjs/transformers/*.js"
    }
  },
  "_size-limit": [
    {
      "path": "lib/AnyExceptionFilter.js",
      "limit": "10kb"
    },
    {
      "path": "lib/AccessLoggerMiddleware.js",
      "limit": "10kb"
    },
    {
      "path": "lib/AnyExceptionFilter.js",
      "limit": "10kb"
    },
    {
      "path": "lib/createNestLogger.js",
      "limit": "10kb"
    },
    {
      "path": "lib/Time.decorator.js",
      "limit": "10kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/nestlibs/nestlib/tree/main/libs/decorators",
  "homepage": "https://github.com/nestlibs/nestlib/tree/main/libs/decorators",
  "bugs": "https://github.com/nestlibs/nestlib/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////",
  "dependencies": {
    "@lsk4/err": "^4.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "*"
  }
}
